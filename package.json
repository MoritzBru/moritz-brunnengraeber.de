{
    "name": "moritz-brunnengraeber.de",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "dev:webpack": "webpack --config webpack.config.js --env.dev --display-error-details",
        "dev:eleventy": "wait-on build/assets/manifest.json && eleventy --watch",
        "dev:serve": "wait-on build/index.html && browser-sync start --config bs-config.js",
        "prod:eleventy": "cross-env ELEVENTY_ENV=production eleventy",
        "prod:webpack": "webpack --config webpack.config.js --env.prod --display-error-details",
        "mjml": "mjml -w ./src/contact/mail.mjml -o ./src/contact/mail.html --config.minify=true",
        "compress": "gzipper --include html,css,js,svg --brotli ./build",
        "clean": "rimraf build",
        "dev": "concurrently -k -c 'bgBlue,bgMagenta,bgCyan' 'npm:dev:webpack' 'npm:dev:eleventy' 'npm:dev:serve'",
        "start": "npm run clean && npm run dev",
        "build": "npm run clean && npm run prod:webpack && npm run prod:eleventy && npm run compress"
    },
    "author": "Moritz Brunnengr√§ber",
    "private": true,
    "devDependencies": {
        "@11ty/eleventy": "^0.10.0",
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "autoprefixer": "^9.7.6",
        "babel-loader": "^8.1.0",
        "browser-sync": "^2.26.7",
        "concurrently": "^5.1.0",
        "cross-env": "^7.0.2",
        "css-loader": "^3.5.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-plugin-import": "^2.20.2",
        "gzipper": "^3.7.0",
        "html-minifier": "^4.0.0",
        "mini-css-extract-plugin": "^0.9.0",
        "mjml": "^4.6.2",
        "node-sass": "^4.13.1",
        "postcss-loader": "^3.0.0",
        "rimraf": "^3.0.2",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.4",
        "stylelint": "^13.3.2",
        "stylelint-config-idiomatic-order": "^8.1.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-scss": "^3.17.0",
        "wait-on": "^5.1.0",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "webpack-manifest-plugin": "^2.2.0"
    },
    "dependencies": {
        "@rqrauhvmra/tobi": "^1.9.1",
        "bootstrap": "^4.4.1",
        "creepyface": "^7.4.2",
        "gsap": "^3.3.4",
        "jquery": "^3.5.1",
        "particles.js": "^2.0.0",
        "popper.js": "^1.16.1",
        "scrollreveal": "^4.0.6"
    }
}
