{
    "name": "moritz-brunnengraeber.de",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "dev:eleventy": "eleventy --watch",
        "dev:webpack": "webpack --config='webpack.config.js' --mode='development' --display-error-details --progress --watch",
        "dev:serve": "browser-sync start --config bs-config.js",
        "prod:eleventy": "eleventy",
        "prod:webpack": "webpack --config='webpack.config.js' --mode='production' --display-error-details --progress",
        "mjml": "mjml -w ./src/contact/mail.mjml -o ./src/contact/mail.html --config.minify=true",
        "compress": "gzipper --include html,css,js,svg --brotli ./build",
        "clean": "rimraf build",
        "start": "npm run clean && concurrently -n '11ty,WEBPACK,SERVE' -c 'bgBlue.bold,bgMagenta.bold,bgCyan.bold' 'npm:dev:eleventy' 'npm:dev:webpack' 'npm:dev:serve'",
        "build": "npm run clean && npm run prod:eleventy && npm run prod:webpack && npm run compress"
    },
    "author": "Moritz Brunnengr√§ber",
    "private": true,
    "devDependencies": {
        "@11ty/eleventy": "^0.10.0",
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "autoprefixer": "^9.7.6",
        "babel-loader": "^8.1.0",
        "browser-sync": "^2.26.7",
        "concurrently": "^5.1.0",
        "css-loader": "^3.5.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-plugin-import": "^2.20.2",
        "gzipper": "^3.7.0",
        "html-minifier": "^4.0.0",
        "mini-css-extract-plugin": "^0.9.0",
        "mjml": "^4.6.2",
        "node-sass": "^4.13.1",
        "postcss-loader": "^3.0.0",
        "rimraf": "^3.0.2",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.4",
        "stylelint": "^13.3.2",
        "stylelint-config-idiomatic-order": "^8.1.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-scss": "^3.17.0",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "@rqrauhvmra/tobi": "^1.9.1",
        "bootstrap": "^4.4.1",
        "creepyface": "^7.4.2",
        "gsap": "^3.3.4",
        "jquery": "^3.5.1",
        "popper.js": "^1.16.1",
        "scrollreveal": "^4.0.6"
    }
}
