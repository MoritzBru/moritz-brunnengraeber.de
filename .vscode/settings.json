{
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "prettier.documentSelectors": ["**/*.astro", "**/*.svg"],
  "[javascript][typescript][json][jsonc][html][astro][svg]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}
